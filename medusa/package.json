{
  "name": "medusa-backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "npx medusa build && ln -sf .medusa/server/public/ public",
    "build:admin": "npx medusa build --admin-only",
    "predeploy": "npx medusa db:migrate",
    "postinstall": "echo 'No admin build needed during postinstall'",
    "seed": "npx medusa exec ./src/scripts/seed.ts",
    "seed:admin": "npx medusa exec ./src/scripts/seed-admin-user.ts",
    "seed:fabrics": "npx medusa exec ./src/scripts/seed-fabrics.ts",
    "import:fabrics": "npx medusa exec ./src/scripts/import-fabric-data.ts",
    "sync:materials": "npx tsx ./src/scripts/sync-materials.ts",
    "test:materials": "npx tsx ./src/scripts/test-materials-sync.ts",
    "setup:contacts": "npx medusa exec ./src/scripts/setup-contacts.ts",
    "test:contacts": "npx medusa exec ./src/scripts/test-contact-integration.ts",
    "setup:us-region": "npx medusa exec ./src/scripts/setup-us-region.ts",
    "setup:inventory": "npx medusa exec ./src/scripts/setup-inventory.ts",
    "seed:bundles": "node ./scripts/seed-bundles.js",
    "fix:stripe-provider": "npx tsx ./scripts/fix-stripe-payment-provider.ts",
    "start": "npx medusa start --host 0.0.0.0",
    "start:prod": "NODE_ENV=production npx medusa start --host 0.0.0.0",
    "dev": "npx medusa develop --host 0.0.0.0",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:integration:materials": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/modules/materials/__tests__/service.integration.spec.ts",
    "test:e2e:materials": "NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/api/admin/materials/__tests__/route.e2e.spec.ts",
    "test:e2e:products": "NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/api/admin/products/__tests__/route.e2e.spec.ts",
    "test:e2e:tax": "NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/api/admin/tax/__tests__/tax.e2e.spec.ts",
    "test:e2e:shipping": "NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/api/store/shipping/__tests__/shipping.e2e.spec.ts",
    "test:e2e:inventory": "NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit src/modules/inventory/__tests__/reservation.e2e.spec.ts",
    "test:order-persistence": "npx tsx ./src/scripts/test-order-persistence.ts",
    "delete:all-orders": "npx tsx ./src/scripts/delete-all-orders.ts",
    "force:delete-orders": "npx tsx ./src/scripts/force-delete-orders.ts",
    "check:delete-orders": "npx tsx ./src/scripts/check-and-delete-orders.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.0",
    "@medusajs/cli": "2.10.0",
    "@medusajs/file-s3": "^2.10.0",
    "@medusajs/framework": "2.10.0",
    "@medusajs/medusa": "2.10.0",
    "@medusajs/notification-sendgrid": "^2.10.0",
    "@medusajs/payment-stripe": "^2.10.0",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "awilix": "^8.0.1",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-parse": "^5.5.6",
    "exceljs": "^4.4.0",
    "multer": "^2.0.2",
    "pg": "^8.13.0",
    "postgres": "^3.4.7",
    "resend": "^6.0.2",
    "stream-json": "^1.8.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@medusajs/medusa-cli": "^1.3.23",
    "@medusajs/test-utils": "2.10.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@types/stream-json": "^1.7.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^2.0.16",
    "@testcontainers/postgresql": "^10.10.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}

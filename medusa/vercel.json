{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/medusa start"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "medusa/medusa-config.js": {
      "maxDuration": 30
    }
  }
}